<div class="i-pagination e-block t-pagination form-inline">
	<h6>Переход по страницам</h6>
	<div class="e-form_row t-pagination form-group">
		<span class="e-prev_elem t-pagination">
			<a class="i-pagination e-prev_btn t-pagination btn btn-secondary {{prev_btn_cls}}"
				href="javascript:void(0)"
				{{prev_btn_attr}}>
				Предыдущая
			</a>
		</span>

		<div class="input-group">
			<span class="e-current_elem t-pagination">
				<input type="text"
					class="i-pagination e-page_num_input t-pagination form-control"
					value="{{cur_page_num}}"
					name="cur_page_num"/>
			</span>
			<span class="e-total_elem t-pagination input-group-addon">
				из {{page_count}}
			</span>

			<span class="input-group-btn">
				<button type="button"
					class="i-pagination e-goto_page_btn t-pagination btn btn-secondary">
					Перейти
				</button>
			</span>
		</div>

		<span class="e-next_elem t-pagination">
			<a class="i-pagination e-next_btn t-pagination btn btn-secondary {{next_btn_cls}}"
				href="javascript:void(0)"
				{{next_btn_attr}}>
				Следующая
			</a>
		</span>
	</div>
</div>